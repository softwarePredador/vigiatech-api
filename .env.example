# Database Connection
# Replace with your actual database credentials from EasyPanel
DATABASE_URL="postgresql://usuario:senha@host:5432/vigiatech-db?schema=public"

# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Secret (Generate a secure random string for production)
JWT_SECRET=your-secret-key-change-in-production

# OpenAI API Configuration (Optional - for LLM integration)
OPENAI_API_KEY=your-openai-api-key-here

# Google Gemini API Configuration (Optional - alternative to OpenAI)
GEMINI_API_KEY=your-gemini-api-key-here

# ML Service URL (if using separate ML service)
ML_SERVICE_URL=http://analista-ml:8000

# Firebase Configuration (Optional - for push notifications)
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY=your-firebase-private-key
FIREBASE_CLIENT_EMAIL=your-firebase-client-email
